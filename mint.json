{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Delphi",
  "logo": {
    "dark": "/logo/logo-color-light-lg.png",
    "light": "/logo/logo-color-dark-sm.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#4091E2",
    "light": "#93CEC7",
    "dark": "#193B4F",
    "anchors": {
      "from": "#4091E2",
      "to": "#93CEC7"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:founders@delphihq.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Log In",
    "url": "https://app.delphihq.com"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Home",
      "icon": "house",
      "url": "https://delphihq.com"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://delphihq.substack.com"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction"]
    },
    {
      "group": "Developers",
      "pages": [
        {
          "group": "SDKs",
          "pages": ["sdks/nodejs"]
        },
        {
          "group": "Frontend Components",
          "pages": ["components/react"]
        }
      ]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoints",
      "pages": [
        {
          "group": "Conversations",
          "pages": ["api-reference/conversations/[id]/get"]
        },
        {
          "group": "Delphi (Natural Language)",
          "pages": [
            "api-reference/delphi/filter-fields/post",
            "api-reference/delphi/query/post",
            "api-reference/delphi/search-entities/post",
            "api-reference/delphi/validated-query/post",
            "api-reference/delphi/workflow/post"
          ]
        },
        {
          "group": "Folders",
          "pages": ["api-reference/folders/get", "api-reference/folders/post"]
        },
        {
          "group": "Query",
          "pages": ["api-reference/query/cube/post"]
        },
        {
          "group": "Saved Queries",
          "pages": [
            "api-reference/saved-queries/get",
            "api-reference/saved-queries/post",
            "api-reference/saved-queries/delete"
          ]
        }
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/delphihq",
    "github": "https://github.com/Delphi-Data",
    "linkedin": "https://www.linkedin.com/company/delphilabs/"
  },
  "api": {
    "baseUrl": "https://app.delphihq.com/api",
    "auth": {
      "method": "key"
    }
  }
}
